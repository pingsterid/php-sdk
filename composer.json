{
    "name": "pingsterid/php-sdk",
    "description": "Official PHP SDK for Pingster.id API",
    "keywords": [
        "pingster",
        "php",
        "sdk",
        "rest",
        "api",
        "openapi"
    ],
    "homepage": "https://pingster.id",
    "license": "MIT",
    "authors": [
        {
            "name": "Pingster.id Dev Team",
            "email": "pingsterid@gmail.com",
            "homepage": "https://pingster.id"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pingster\\": "lib/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Pingster\\Test\\": "test/"
        }
    }
}